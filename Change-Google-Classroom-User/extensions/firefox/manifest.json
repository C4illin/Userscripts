{
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/ChangeClassroom.user.js" ],
      "matches": [ "https://classroom.google.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Switch Google Classroom user automatically depending on who has access",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
   },
   "manifest_version": 2,
   "name": "Change User for Google Classroom",
   "short_name": "Change User Classroom",
   "version": "1.2.2",
   "web_accessible_resources": [ "js/ChangeClassroom.user.js" ]
}
